{
  "name": "omnidrive",
  "version": "1.0",
  "manifest_version": 2,
  "omnibox": {
    "keyword": "new"
  },
  "icons": {
    "16": "product16.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
